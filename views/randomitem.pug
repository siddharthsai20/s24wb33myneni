extends layout
 
block content
    h1= title
    - var image_names = ['image1.jpg', 'image2.jpg', 'image3.jpg', 'image4.jpg', 'image5.jpg'];
    - for (let i = 0; i < 3; i++)
        - var randomIndex = Math.floor(Math.random() * 5);
        img(src=`/images/${image_names[randomIndex]}`, alt=`Image ${i + 1}`, height='300', width='300')